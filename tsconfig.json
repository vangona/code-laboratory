{
  "compilerOptions": {
    "target": "es6",
    "module": "ES6",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "rootDir": "assets/js",
    "outDir": "dist",
    "moduleResolution": "node"
  },
  "include": ["assets/js/**/*.ts"],
  "exclude": ["node_modules"]
}
